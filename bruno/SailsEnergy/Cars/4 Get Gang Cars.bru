meta {
  name: 4 Get Gang Cars
  type: http
  seq: 5
}

get {
  url: {{baseUrl}}/api/gangs/{{gangId}}/cars
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

script:post-response {
  if (res.status === 200 && res.body.length > 0) {
    bru.setVar("gangCarId", res.body[0].id);
  }
}
