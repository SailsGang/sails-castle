# Docker Environment Variables
# Copy this file to .env and fill in your secrets
# DO NOT commit .env to git!

# Database
POSTGRES_USER=sails
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=sailsenergy

# RabbitMQ
RABBITMQ_USER=sails
RABBITMQ_PASSWORD=your_secure_password_here

# JWT (must be at least 32 characters)
JWT_SECRET=your_super_secret_jwt_key_at_least_32_chars

# Seq
SEQ_ADMIN_PASSWORD=your_secure_password_here
